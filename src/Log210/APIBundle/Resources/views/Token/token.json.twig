{
    "access_token": "{{ token.id }}",
    "refresh_token": "{{ token.refreshToken }}",
    "expires_in": {{ token.expirationDate|date("U") - "now"|date("U") }},
    "expiration_date": "{{ token.expirationDate|date("Y-m-d H:i:s") }}",
    "user_type": "{{ user_type }}",
    "links": [
        {
            "rel": "self",
            "href": "/api/tokens/{{ token.id }}"
        }
    ]
}
